      "source": [
        "# Setup ngrok tunnel for GUI access\n",
        "from pyngrok import ngrok\n",
        "import threading\n",
        "import subprocess\n",
        "import time\n",
        "import webbrowser\n",
        "\n",
        "# Create ngrok tunnel\n",
        "tunnel = ngrok.connect(8188)\n",
        "ngrok_url = tunnel.public_url\n",
        "\n",
        "print(f'üåê ComfyUI GUI accessible at: {ngrok_url}')\n",
        "print(f'üìã Open this URL in your browser: {ngrok_url}')\n",
        "print('\nü¶ç Ready to generate gorilla images!')"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {},
      "outputs": [],
      "source": [
        "# Start ComfyUI with GUI\n",
        "def start_comfyui():\n",
        "    subprocess.run(['python', 'main.py', '--listen', '0.0.0.0', '--port', '8188'], cwd='ComfyUI')\n",
        "\n",
        "# Start ComfyUI in background\n",
        "comfyui_thread = threading.Thread(target=start_comfyui)\n",
        "comfyui_thread.daemon = True\n",
        "comfyui_thread.start()\n",
        "\n",
        "# Wait for startup\n",
        "time.sleep(30)\n",
        "print('‚úÖ ComfyUI started!')\n",
        "print(f'üåê GUI Access: {ngrok_url}')\n",
        "print('\nüìù Open the URL above in your browser to use ComfyUI!')\n",
        "print('ü¶ç Load the gorilla workflow and generate images!')"
      ]
    },
